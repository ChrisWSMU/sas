			<string>keyword.control.sas</string>
		</dict>
		<dict>
			<key>captures</key>
			<dict>
				<key>2</key>
				<dict>
					<key>name</key>
					<string>support.function.sas</string>
				</dict>
				<key>3</key>
				<dict>
