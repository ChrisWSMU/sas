		<dict>
			<key>include</key>
			<string>#star_comment</string>
		</dict>
		<dict>
			<key>include</key>
			<string>#hanging_comment</string>
		</dict>
		<dict>
			<key>begin</key>
			<string>/\*</string>
			<key>end</key>
